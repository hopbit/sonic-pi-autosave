# empty buffer

# Filtered Dnb

# Coded by Sam Aaron

use_sample_bpm :loop_amen

with_fx :rlpf, cutoff: 10, cutoff_slide: 4 do |c|
  live_loop :dnb do
    with_fx :flanger do
      sample :bass_dnb_f, amp: 5
    end
    with_fx :wobble, phase: 0.5 do
      sample :loop_amen, amp: 5
    end
    sleep 1
    control c, cutoff: rrand(80, 120), cutoff_slide: rrand(2, 8)
  end
end